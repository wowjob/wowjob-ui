{
  "name": "@wowjob/ui",
  "version": "0.0.2",
  "description": "A collection of reusable React components for the WowJob.ai ecosystem, built for React 19.",
  "author": "Marian Zburlea (https://github.com/marianzburlea)",
  "license": "MIT",
  "homepage": "https://github.com/wowjob/wowjob-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wowjob/wowjob-ui.git"
  },
  "bugs": {
    "url": "https://github.com/wowjob/wowjob-ui/issues"
  },
  "keywords": [
    "wowjob",
    "ui",
    "react",
    "react-components",
    "design-system",
    "ui-kit",
    "react19"
  ],
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
