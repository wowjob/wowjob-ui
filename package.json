    "dev": "tsup --watch"
  },
  "dependencies": {
    "@tanstack/react-form": "^1.12.1",
    "@wowjob/css": "latest",
    "@wowjob/type": "latest",
    "@wowjob/util": "latest",
    "rehype-react": "^8.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "zod": "^4.1.12"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "eslint": "^9.25.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "semantic-release": "^24.2.9",
    "tsup": "^8.5.0",
    "typescript": "^5.8.2"
  }
}
